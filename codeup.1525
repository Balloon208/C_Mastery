#include <stdio.h>

int main()
{
    int i, j, k, n;
    int num[22][22] = {}; //입력받을 보드판
    int result[22][22] = {}; //출력할 보드판
    
    for(i=0; i<10; i++) //10*10 보드판 입력받기
    {
        for(j=0; j<10; j++)
        {
            scanf("%d", &num[i][j]);
            result[i][j] = num[i][j]; //result 배열에 복사
            
        }
    }
    
    scanf("%d", &n); //플레이어 수 입력받기
    
    int player[9][4] = {0}; //플레이어 순서, 좌표, 생존여부 배열
    
    for(i=0; i<n; i++) //플레이어 좌표 입력받기
    {
        scanf("%d %d", &player[i][0], &player[i][1]);
        player[i][0]--;
        player[i][1]--;
    }
    
    for(i=0; i<10; i++) //물풍선 터뜨리기
    {
        for(j=0; j<10; j++)
        {
            if(num[i][j]>=1) //물풍선이 있을 때
            {
                result[i][j] = -2;
                
                for(k=1; k<=num[i][j]; k++) //오른쪽
                {
                    if(num[i][j+k] == -1)
                        break;
                    result[i][j+k] = -2;
                }
                for(k=1; k<=num[i][j]; k++) //왼쪽
                {
                    if(num[i][j-k] == -1)
                        break;
                    result[i][j-k] = -2;
                }
                for(k=1; k<=num[i][j]; k++) //아래쪽
                {
                    if(num[i+k][j] == -1)
                        break;
                    result[i+k][j] = -2;
                }
                for(k=1; k<=num[i][j]; k++) //위쪽
                {
                    if(num[i-k][j] == -1)
                        break;
                    result[i-k][j] = -2;
                }
                
            }
        }
    }
    
    for(i=0; i<n; i++) //플레이어 생존여부 저장 
    {
        if(result[player[i][0]][player[i][1]] == 0) //플레이어가 생존했을 때
        {
            result[player[i][0]][player[i][1]] = i+1;
            player[i][2] = 1; 
        }
    }
    
    for(i=0; i<10; i++) //결과 보드판 출력
    {
        for(j=0; j<10; j++)
        {
            printf("%d ", result[i][j]);
        }
        printf("\n");
    }
    
    printf("Character Information\n");
    
    for(i=0; i<n; i++) //플레이어 생존여부에 따른 텍스트 출력
    {
        if(player[i][2] == 1) //생존 시
            printf("player %d survive\n", i+1);
        else //생존하지 않았을 시
            printf("player %d dead\n", i+1);
    }
    
    return 0;
}

